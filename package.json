{
  "name": "sample-editor-view",
  "version": "1.0.0",
  "description": "A Canvas Renderer / Editor UI for AudioBuffers",
  "main": "dist/SampleEditorView.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "./node_modules/.bin/karma start --single-run",
    "test:mocha": "mocha-phantomjs",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rikard-io/SampleEditorView.git"
  },
  "keywords": [
    "WebAudio",
    "Waveform",
    "Sampler",
    "DAW"
  ],
  "author": "Rikard Lindstrom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rikard-io/SampleEditorView/issues"
  },
  "homepage": "https://github.com/rikard-io/SampleEditorView",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.9",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "yargs": "^10.0.3",
  }
}
